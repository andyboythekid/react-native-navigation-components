{
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "node": true
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0,
    "react/no-children-prop": 0,
    "react/display-name": 0,
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "settings": {
    "react": {
      "version": "16.6.3"
    }
  }
}
